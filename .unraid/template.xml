<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>OmbiBot</Name>
  <Repository>thegorgotron83/ombibot</Repository>
  <Registry>https://hub.docker.com</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/marjoemitchell/Ombibot/issues</Support>
  <Project>https://github.com/marjoemitchell/Ombibot</Project>
  <Overview>A Slack bot for Ombi request management</Overview>
  <Category>Automation</Category>
  <WebUI>http://[IP]:[PORT:PORT]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/marjoemitchell/Ombibot/master/.unraid/template.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/marjoemitchell/Ombibot/master/ombibot.jpeg</Icon>
  <ExtraParams></ExtraParams>
  <PostArgs></PostArgs>
  <CPUset></CPUset>
  <DateInstalled></DateInstalled>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <Description>OmbiBot is a Slack bot that integrates with Ombi for media request management. Users can search and request movies via Slack commands.</Description>
  <Environment>
    <Variable>
      <Name>SLACK_API_TOKEN</Name>
      <Value></Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>OMBI_URL</Name>
      <Value>http://192.168.1.5:3579</Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>OMBI_API_KEY</Name>
      <Value></Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>PORT</Name>
      <Value>3000</Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>TZ</Name>
      <Value>America/Denver</Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>PLEX_URL</Name>
      <Value>http://plex:32400</Value>
      <Mode></Mode>
    </Variable>
    <Variable>
      <Name>PLEX_TOKEN</Name>
      <Value></Value>
      <Mode></Mode>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3000</HostPort>
        <ContainerPort>3000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Labels></Labels>
</Container>
